<h4>timed_script2.parsed_paragraphs</h4>
<table class="bordered">
  <tr>
    <th>No.</th>
    <th>parsed_paragraph</th>
    <th>processed_paragraph.slices</th>
    <th>processed_paragraph (segments)</th>
  </tr>

  <% o.parsed_paragraphs.each_with_index do |parsed_paragraph, index| %>
    <tr>
      <td><%= index + 1 %></td>

      <% processed_paragraph = o.process_paragraph(parsed_paragraph) %>

      <td><%= render_object parsed_paragraph %></td>

      <td>
        <table class="codes">
          <% processed_paragraph.slices.each do |slice| %>
            <tr>
              <td><code><%= slice[0] %></code></td>
              <td><%= render_precise_timestamp slice[1] %></td>
              <td><code><%= slice[2] %></code></td>
            </tr>
          <% end %>
        </table>
      </td>

      <td>
        <%= render_object processed_paragraph %>
      </td>
    </tr>
  <% end %>
</table>
